{
  "name": "funish-games",
  "version": "1.0.0",
  "description": "Real-time multiplayer games platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "express": "^4.21.2",
    "socket.io": "^4.8.3",
    "vite": "^6.2.0"
  },
  "build": {
    "commands": [
      "npm install",
      "npm run build"
    ]
  },
  "start": {
    "commands": [
      "npm start"
    ]
  }
}
